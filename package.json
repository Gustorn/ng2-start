  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "serve:dev": "webpack-dev-server --progress --colors --display-error-details --display-cached --inline --open",
    "build:dev": "webpack --progress --colors --display-error-details --display-cached",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.9",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.7",
    "typings": "^0.7.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
