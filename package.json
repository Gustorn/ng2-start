{
  "name": "ng2-start",
  "version": "0.0.0",
  "private": true,
  "author": "Felix Itzenplitz",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "serve:dev": "webpack-dev-server --progress --colors --display-error-details --display-cached --inline --open",
    "build:dev": "webpack --progress --colors --display-error-details --display-cached",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.15",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "^0.6.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.7.11",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}
