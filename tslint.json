{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false
    ]
  },
  "rulesDirectory": []
}
